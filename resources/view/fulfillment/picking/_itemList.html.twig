
<div class="page">
	<table>
		<thead>
		<tr>
			<th>Brand</th>
			<th>Description</th>
			<th>Barcode</th>
			<th>Quantity</th>
		</tr>
		</thead>
		<tbody>

			{% if items | length == 0 %}
				{% set items = order['items'] %}
			{% endif %}

			{% for item in items %}
				<tr>
					<td>{{ item['item'].brand }}</td>
					<td>{{ item['item'].productName }}</td>
					<td>{{ item['item'].barcode }}</td>
					<td>{{ item['quantity'] }}</td>
				</tr>
			{% endfor %}

		</tbody>
	</table>
</div>