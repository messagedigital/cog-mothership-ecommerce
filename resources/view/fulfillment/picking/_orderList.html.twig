
<div class="page first">
	<table class="manifest">
		<thead>
		<tr>
			<th>Date/Time</th>
			<th>Name</th>
			<th>Order</th>
			<th>Shipping method</th>
			<th>Value</th>
		</tr>
		</thead>
		<tbody>
		{% for key, order in orders %}
			<tr>
				<td>{{ order['order'].authorship.createdAt.date }}</td>
				<td>{{ order['order'].user.name }}</td>
				<td>{{ order['order'].id }}</td>
				<td>{{ order['order'].shippingName }}</td>
				<td>{{ order['order'].totalNet }}</td>
			</tr>
		{% endfor %}
		</tbody>
	</table>
</div>